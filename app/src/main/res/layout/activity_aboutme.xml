<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/aboutme_accountDetails"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutmeActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="239dp"
        android:layout_height="54dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="70dp"
        android:layout_marginEnd="124dp"
        android:layout_marginBottom="11dp"
        android:text="Account Details"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/about_name2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/about_name2"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="184dp"
        android:text="Name"
        android:textSize="17sp"
        app:layout_constraintEnd_toStartOf="@+id/about_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="58dp"
        android:layout_height="32dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="136dp"
        android:text="Email"
        android:textSize="17sp"
        app:layout_constraintEnd_toStartOf="@+id/about_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/about_name2" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="75dp"
        android:layout_height="34dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="201dp"
        android:text="Balance"
        android:textSize="17sp"
        app:layout_constraintEnd_toStartOf="@+id/about_balance"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/about_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="184dp"
        android:layout_marginBottom="21dp"
        android:text="Raditya"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/about_email"
        app:layout_constraintStart_toEndOf="@+id/about_name2" />

    <TextView
        android:id="@+id/about_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="136dp"
        android:layout_marginBottom="29dp"
        android:text="Raditya@netlab.com"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/about_balance"
        app:layout_constraintStart_toEndOf="@+id/textView5" />

    <TextView
        android:id="@+id/about_balance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="26dp"
        android:text="0.0"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="197dp"
        android:layout_height="48dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="54dp"
        android:ems="10"
        android:hint="Amount"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <Button
        android:id="@+id/button"
        android:layout_width="124dp"
        android:layout_height="48dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="38dp"
        android:text="TOP UP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView6" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="20dp" />

    <Button
        android:id="@+id/aboutme_registerRenter"
        android:layout_width="285dp"
        android:layout_height="49dp"
        android:layout_marginTop="9dp"
        android:text="REGISTER RENTER"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

    <androidx.cardview.widget.CardView
        android:id="@+id/registerCardView"
        android:layout_width="384dp"
        android:layout_height="294dp"
        android:layout_marginStart="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="wrap_content"
            android:layout_height="294dp">

            <EditText
                android:id="@+id/aboutme_name2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:ems="10"
                android:hint="Name"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/aboutme_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="17dp"
                android:ems="10"
                android:hint="Address"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/aboutme_name2" />

            <EditText
                android:id="@+id/aboutme_phoneNumber"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="19dp"
                android:ems="10"
                android:hint="Phone Number"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/aboutme_address" />

            <Button
                android:id="@+id/aboutme_register"
                android:layout_width="147dp"
                android:layout_height="48dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="24dp"
                android:text="REGISTER"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/aboutme_phoneNumber" />

            <Button
                android:id="@+id/aboutme_cancel"
                android:layout_width="147dp"
                android:layout_height="48dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="20dp"
                android:text="CANCEL"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/aboutme_phoneNumber" />

            <androidx.cardview.widget.CardView
                android:id="@+id/dataCardView"
                android:layout_width="420dp"
                android:layout_height="272dp"
                android:layout_marginTop="30dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="156dp">

                    <TextView
                        android:id="@+id/aboutme_textviewName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="20dp"
                        android:text="Name"
                        android:textSize="17sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/aboutme_textviewStore"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="197dp"
                        android:layout_marginTop="20dp"
                        android:text="Netlab"
                        android:textSize="17sp"
                        app:layout_constraintStart_toEndOf="@+id/aboutme_textviewName"
                        app:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/aboutme_textviewAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="14dp"
                        android:text="Address"
                        android:textSize="17sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/aboutme_textviewName" />

                    <TextView
                        android:id="@+id/aboutme_textviewPlace"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="144dp"
                        android:layout_marginTop="13dp"
                        android:text="Gedung DTE Lantai 2"
                        android:textSize="16sp"
                        app:layout_constraintStart_toEndOf="@+id/aboutme_textviewAddress"
                        app:layout_constraintTop_toBottomOf="@+id/aboutme_textviewStore" />

                    <TextView
                        android:id="@+id/aboutme_textviewPhoneNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="30dp"
                        android:layout_marginTop="27dp"
                        android:text="Phone Number"
                        android:textSize="17sp"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/aboutme_textviewAddress" />

                    <TextView
                        android:id="@+id/aboutme_textviewNumber"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="112dp"
                        android:layout_marginTop="28dp"
                        android:text="081234567890"
                        android:textSize="17sp"
                        app:layout_constraintStart_toEndOf="@+id/aboutme_textviewPhoneNumber"
                        app:layout_constraintTop_toBottomOf="@+id/aboutme_textviewPlace" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>